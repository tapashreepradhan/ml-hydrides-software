{
    "name": "Python Dev Container",
    "dockerFile": "Dockerfile",
    "context": "..",
    "settings": {
      "python.pythonPath": "/usr/local/bin/python"
    },
    "extensions": [
      "ms-python.python"
    ],
    "postCreateCommand": "pip install --no-cache-dir -r requirements.txt",
    "remoteUser": "vscode"
  }
